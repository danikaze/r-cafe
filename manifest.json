{
  "manifest_version": 2,

  "name": "Яakuten Cafeteria Menu",
  "short_name": "ЯakutenCafe",
  "description": "Shows a preview of the Яakuten Crimson House Cafeteria menus",
  "version": "0.8.0",

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png"
  },

  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_popup": "index.html",
    "default_title": "Show Яakuten Cafeteria menu"
  },
  "permissions": ["http://*/*", "https://*/*"]
}
